{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-tl",
                "${workspaceFolder}/ConsoleApp1/ConsoleApp1.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "building"
                    }
                }
            }
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ConsoleApp1/ConsoleApp1.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "publising"
                    }
                }
            }
        },
        {
            "label": "pack",
            "command": "dotnet",
            "type": "process",
            "args": [
                "pack",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "packing"
                    }
                }
            }
        }
    ]
}
