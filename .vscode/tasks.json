{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-tl",
                "${workspaceFolder}/ConsoleApp1/ConsoleApp1.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "hide": true
                }
            }
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ConsoleApp1/ConsoleApp1.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "hide": true
                }
            }
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/ConsoleApp1/ConsoleApp1.csproj"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "hide": true
                }
            }
        },
        {
            "label": "clean Nuget",
            "command": "dotnet",
            "type": "process",
            "args": [
                "nuget",
                "locals",
                "all",
                "--clear",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "Cleaning"
                    }
                }
            }
        },
        {
            "label": "pack",
            "command": "dotnet",
            "type": "process",
            "args": [
                "pack",
                "-tl",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "Packing"
                    }
                }
            }
        },
        {
            "label": "restore",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "-tl",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "Restoring"
                    }
                }
            }
        },
        {
            "label": "build all",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-tl",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "Building"
                    }
                }
            }
        },
        {
            "label": "test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test",
                "-tl",
                "${workspaceFolder}/PrimaryParameter.Tests/PrimaryParameter.Tests.csproj",
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "statusbar": {
                    "running": {
                        "label": "Testing"
                    }
                }
            }
        },
    ]
}
